{
  "name": "shub",
  "description": "SwaggerHub CLI",
  "version": "0.0.0",
  "author": "SmartBear",
  "bin": {
    "shub": "./bin/run"
  },
  "bugs": "https://github.com/SmartBear/swaggerhub-cmd/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/SmartBear/swaggerhub-cmd",
  "keywords": [
    "oclif"
  ],
  "license": "ASF2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "shub",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "SmartBear/swaggerhub-cmd",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}

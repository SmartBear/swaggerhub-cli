{
  "name": "swaggerhub",
  "description": "SwaggerHub CLI",
  "version": "0.2.0",
  "author": "SmartBear",
  "bin": {
    "swaggerhub": "./bin/run"
  },
  "bugs": "https://github.com/SmartBear/swaggerhub-cmd/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^3.0.1",
    "deep-extend": "^0.6.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "js-yaml": "^3.13.1",
    "json-templates": "^3.0.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.0",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint_d": "^8.1.1",
    "rewire": "^5.0.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-mocha": "^6.3.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nock": "^12.0.3",
    "nyc": "^14.1.1",
    "tmp": "^0.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/SmartBear/swaggerhub-cmd",
  "keywords": [
    "oclif"
  ],
  "license": "Apache 2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "hooks": {
      "init": "./src/hooks/setup-config"
    },
    "bin": "swaggerhub",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "configDefaults": {
      "SWAGGERHUB_URL": "https://api.swaggerhub.com"
    },
    "topics": {
      "api": {
        "description": "this topic supports the management of APIs"
      },
      "api:version": {
        "description": "this topic supports the management of individual versions of an API"
      }
    }
  },
  "repository": "SmartBear/swaggerhub-cmd",
  "scripts": {
    "github-release": "release-it -VV --config ./release/.release-it.json",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "lint": "eslint_d ./src ./test",
    "postinstall": "node ./src/check-node-version",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  }
}

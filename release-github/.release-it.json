{
  "hooks": {},
  "git": {
    "changelog": "./release-github/get-changelog.sh",
    "commitMessage": "release: ${version}",
    "tagName": "${version}",
    "push": false
  },
  "github": {
    "release": true,
    "releaseName": "SwaggerHub CLI %s Released!",
    "releaseNotes": "./release-github/get-changelog.sh",
    "draft": true
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular"
    }
  }
}

{
  "hooks": {},
  "git": {
    "requireUpstream": false
  },
  "github": {
    "release": true,
    "releaseName": "SwaggerHub CLI %s Released!",
    "releaseNotes": "./release-github/get-changelog.sh",
    "tokenRef": "$GITHUB_TOKEN",
    "draft": true
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular"
    }
  }
}
